<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.proyectopoo.InfoFeriaController">
   <children>
      <VBox fx:id="dx" alignment="TOP_CENTER" prefHeight="450.0" prefWidth="700.0">
         <children>
            <Label text="Info Feria" />
            <HBox alignment="CENTER" prefHeight="74.0" prefWidth="700.0">
               <children>
                  <Label text="Feria" />
                  <ComboBox fx:id="feria" prefWidth="150.0" />
               </children>
            </HBox>
            <Button fx:id="consulta" mnemonicParsing="false" onAction="#consultar" text="consultar" />
            <Button fx:id="reg" mnemonicParsing="false" onAction="#regresar" text="Regrasar" />
         </children>
      </VBox>
   </children>
</AnchorPane>
